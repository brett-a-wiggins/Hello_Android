<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hello_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainVehicleMenuActivity"></activity>
        <activity android:name=".SelectVehicleActivity"></activity>
        <activity android:name=".ExistingUserActivity"></activity>
        <activity android:name=".NewUserActivity"></activity>
        <activity android:name=".AddNewCar"></activity>
        <activity android:name=".AddNewVehicle"></activity>
        <activity android:name=".OdometerActivity" />
        <activity android:name=".ServiceActivity" />
        <activity android:name=".FillUPActivity" />
        <activity android:name=".VehicleMenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>